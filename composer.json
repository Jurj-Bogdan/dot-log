{
    "name": "dotkernel/dot-log",
    "type": "library",
    "description": "DotKernel log component extending and customizing laminas-log",
    "license": "MIT",
    "homepage": "https://github.com/dotkernel/dot-log",
    "keywords": [
      "log",
      "logging",
      "services",
      "mezzio",
      "laminas",
      "laminas-log"
    ],
    "authors": [
        {
            "name": "DotKernel Team",
            "email": "team@dotkernel.com"
        }
    ],
    "require": {
        "php": "~7.4.0 || ~8.0.0 || ~8.1.0",
        "psr/http-message": "^1.0.1",
        "laminas/laminas-servicemanager": "^3.10.0",
        "laminas/laminas-log": "2.15.2",
        "laminas/laminas-dependency-plugin": "^2.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1.2",
        "squizlabs/php_codesniffer": "^3.5.5",
        "dotkernel/dot-mail": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
            "Dot\\Log\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DotTest\\Log\\": "test/"
        }
    }
}
